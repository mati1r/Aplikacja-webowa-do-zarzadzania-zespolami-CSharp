@model List<MessagesModel.ReciveMessageView>
<hr/>
@foreach(var item in Model){
    <div class="recive-item card-body" id="@item.message_id">
        <div class="contain">
            <div class="row">
                <div class="col-md-2">
                    Od: @item.sender_name
                </div>
                <div class="col-md-7">
                    Temat: @item.topic
                </div>
                <div class="col-md-3">
                    Data wysłania: @item.send_date
                </div>
            </div>
        </div>
    </div>
    <hr />
}
<button class="btn btn-dark w-100" id="loadMoreRecived">Pokaż więcej</button>
